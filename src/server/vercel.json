{
  "version": 2,
  "builds": [
    {
      "src": "server/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "//server/server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
  },
  "installCommand": "npm install",
  "buildCommand": "npm run build"
}
